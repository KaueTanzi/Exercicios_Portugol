programa {
  funcao inicio() {
    
    real num1,num2
    escreva("informe dois valores para as quatro operaçoes: \n")
    leia(num1)
    leia(num2)
    escreva("adicao:",num1+num2,"\n")
    escreva("subtraçao:",num1-num2,"\n")
